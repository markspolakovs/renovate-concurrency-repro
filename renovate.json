{
    "customDatasources": {
        "sample": {
            "defaultRegistryUrlTemplate": "https://raw.githubusercontent.com/markspolakovs/renovate-concurrency-repro/9d286e1a7cd107edcd345ef0ec8137a31f5ddcf2/custom-data.json",
            "format": "json",
            "transformTemplates": [
                "$$.assets.links[name.$contains(\"{{{packageName}}}\")].name.$replace(/^.*_(.+?)_.*$/, '$1')",
                "{ 'releases': $map($$, function($r) { { 'version': $r } }) }"
            ]
        }
    },
    "customManagers": [
        {
            "customType": "regex",
            "managerFilePatterns": ["sample-custom-manager.yaml"],
            "matchStrings": [
                "# renovate: datasource=(?<datasource>\\S+?)\\n\\s*(?<depName>[^:#]+?):\\s*?['\"]?(?<currentValue>\\S+?)['\"]?[\\s$]"
            ]
        }
    ]
}
